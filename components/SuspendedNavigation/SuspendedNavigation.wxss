/*碳层  */

.modalbg {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 1000;
}

.img-plus-style {
  height: 80rpx;
  width: 80rpx;
  position: fixed;
  bottom: 200rpx;
  right: 30rpx;
  z-index: 10010;
  opacity: 0.7;
  border-radius: 100px;
}

.img-style {
  height: 60rpx;
  width: 60rpx;
  padding: 10rpx;
  position: fixed;
  bottom: 200rpx;
  right: 30rpx;
  opacity: 0;
  z-index: 1000;
  background-color: white;
  border-radius: 100px;
  display: flex;
  justify-content: center;
  align-items: center;
}

.img-style image {
  height: 50rpx;
  width: 50rpx;
}

.img-plus-style-active {
  height: 60rpx;
  width: 60rpx;
  background-color: #9c855c;
  opacity: 1;
  padding: 10rpx;
}
