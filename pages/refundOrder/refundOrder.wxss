

page {
  background-color: #eee;
}


.order-list {
  width: 750rpx;
}

.nomore {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 10pt;
  color: #aaa;
  padding: 20rpx 0;
}

.noresult {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  font-size: 10pt;
  color: #aaa;
}

.noresult image {
  width: 200rpx;
  height: 200rpx;
  margin: 200rpx 0 0 0;
}

.noresult navigator:nth-child(3) {
  color: #9c855c;
  margin: 10rpx 0 0 0;
  text-decoration: underline;
}
